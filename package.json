{
  "name": "jxlvuetpl",
  "private": true,
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:jxlust/JxlVueTpl.git"
  },
  "keywords": [
    "Vite",
    "Vue3",
    "TypeScript"
  ],
  "author": "jxlust",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "https://jxlust.github.io/JxlVueTpl",
  "engines": {
    "node": "^12 || >=14"
  },
  "scripts": {
    "bootstrap": "pnpm install",
    "serve": "npm run dev",
    "dev": "cross-env --max_old_space_size=4096 vite",
    "dev:p2": "vite serve src/pager2/ --config ./vite.config.ts",
    "dev:pc": "cross-env NODE_ENV=pc --max_old_space_size=4096 vite",
    "build": "cross-env vite build",
    "build:dev": "cross-env vite build --mode development",
    "build:lib": "node ./script/build.js",
    "sit": "cross-env vite build --mode sit",
    "build:no-cache": "pnpm clean:cache && npm run build",
    "build-tsc": "vue-tsc --noEmit && vite build",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged --concurrent false",
    "deploy": "npm run build && npx gh-pages -d dist",
    "test:gzip": "npx http-server dist --cors --gzip -c-1",
    "test:br": "npx http-server dist --cors --brotli -c-1",
    "reinstall": "rimraf pnpm-lock.yaml && rimraf package.lock.json && rimraf node_modules && npm run bootstrap",
    "prepare": "husky install",
    "commit": "cz",
    "test": "vitest",
    "test:c": "vitest run --coverage",
    "create:version": "node ./script/createVersion.js"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.0-beta.0",
    "@vant/touch-emulator": "^1.3.2",
    "@vueuse/core": "^8.5.0",
    "@vueuse/integrations": "^8.5.0",
    "@wangeditor/editor": "^5.1.14",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "animate.css": "^4.1.1",
    "axios": "^0.27.2",
    "dayjs": "^1.11.2",
    "dexie": "^3.2.2",
    "echarts": "^5.4.2",
    "element-plus": "^2.3.7",
    "gesto": "^1.19.1",
    "lodash-es": "^4.17.21",
    "monaco-editor": "^0.33.0",
    "nanoid": "^3.3.4",
    "normalize.css": "^8.0.1",
    "pinia": "^2.0.14",
    "qrcode": "^1.5.0",
    "qs": "^6.10.3",
    "tributejs": "^5.1.3",
    "vant": "^3.4.9",
    "vform3-builds": "^3.0.10",
    "vue": "^3.2.25",
    "vue-i18n": "^9.2.2",
    "vue-request": "^2.0.3",
    "vue-router": "^4.0.15",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@intlify/unplugin-vue-i18n": "^0.12.2",
    "@testing-library/vue": "^6.6.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/compiler-sfc": "^3.2.36",
    "autoprefixer": "^10.4.7",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.9",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.0.1",
    "filemanager-plugin": "^2.8.1",
    "happy-dom": "^8.9.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.2",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.14",
    "postcss-html": "^1.4.1",
    "postcss-px-to-viewport-8-plugin": "^1.1.3",
    "prettier": "^2.6.2",
    "sass": "^1.52.1",
    "stylelint": "^14.8.5",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.2.0",
    "terser": "^5.15.1",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.8.5",
    "unplugin-vue-components": "^0.19.5",
    "unplugin-vue-define-options": "^0.6.1",
    "uuid": "^8.3.2",
    "vconsole": "^3.14.6",
    "vite": "^4.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-dts": "^1.6.6",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-windicss": "^1.8.4",
    "vitest": "^0.34.1",
    "vue-eslint-parser": "^9.0.2",
    "vue-tsc": "^0.34.7",
    "windicss": "^3.5.4"
  },
  "peerDependencies": {
    "postcss": "^8.4.14"
  },
  "lint-staged": {
    "src/**/*.{ts,js,vue,tsx,jsx}": [
      "prettier --write --cache --ignore-unknown",
      "eslint --cache --fix"
    ]
  }
}
